{
	"name": "resume-builder",
	"version": "0.0.1",
	"scripts": {
		"dev:linux": "sudo chmod -R ugo+rwx .nhost && npm run dev",
		"dev": "svelte-kit dev",
		"schema:apollo": "npm run apollo schema:download --endpoint http://localhost:1337/v1/graphql --header \"X-Hasura-Admin-Secret: nhost-admin-secret\"",
		"schema": "gq http://localhost:1337/v1/graphql --header \"X-Hasura-Admin-Secret: nhost-admin-secret\" --introspect > ./src/lib/graphql/schema.graphql",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"test": "jest",
		"test:watch": "npm run test -- --watchAll",
		"test:coverage": "jest --coverage",
		"pwtest": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@iconify-json/heroicons-outline": "^1.1.1",
		"@iconify-json/mdi": "^1.1.0",
		"@playwright/test": "^1.21.0-alpha-mar-31-2022",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-netlify": "^1.0.0-next.47",
		"@sveltejs/kit": "next",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@testing-library/jest-dom": "^5.16.2",
		"@testing-library/svelte": "^3.0.3",
		"@types/cookie": "^0.4.1",
		"@types/jest": "^27.4.1",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.2",
		"broadcast-channel": "^4.10.0",
		"cssnano": "^5.0.17",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"jest": "^27.5.1",
		"postcss": "^8.4.6",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.2.6",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.0.23",
		"ts-jest": "^27.1.3",
		"tslib": "^2.3.1",
		"typescript": "~4.5.4",
		"unplugin-icons": "^0.13.2"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@lukeed/uuid": "^2.0.0",
		"@nhost/nhost-js": "^0.3.10",
		"@tailwindcss/forms": "^0.4.0",
		"@tailwindcss/typography": "^0.5.2",
		"chrome-aws-lambda": "^10.1.0",
		"classcat": "^5.0.3",
		"cookie": "^0.4.1",
		"dayjs": "^1.11.0",
		"jsonpath-plus": "^6.0.1",
		"playwright": "^1.20.2",
		"playwright-aws-lambda": "^0.7.0",
		"playwright-core": "^1.20.2"
	}
}
